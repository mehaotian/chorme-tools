{
  "manifest_version": 3,
  "name": "CRXJS Vanilla JS Example",
  "version": "1.0.0",
  "action": { "default_popup": "index.html" },
  "side_panel": {
    "default_path": "slider.html"
  },
  "permissions": [
    "storage",
    "alarms",
    "scripting",
    "activeTab",
    "bookmarks",
    "notifications",
    "sidePanel"
  ]
}
